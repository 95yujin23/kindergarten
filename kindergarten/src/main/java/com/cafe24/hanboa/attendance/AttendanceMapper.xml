	<select id="selectAllKidsattendance" resultType="com.cafe24.hanboa.attendance.KidsAttendance">
		SELECT
				attendance_cd 			AS attendanceCd
				,kids_cd 				AS kidsCd
				,kids_attendance_time 	AS kidsAttendanceTime
				,kids_come_home_time 	AS kidsComeHomeTime
				,kids_rest_late_type 	AS kidsRestLateTime
